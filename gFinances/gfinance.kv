<Manager>:
    Menu:
        name: 'menu'
    Finances:
        name: 'finances'
<Menu>:
    BoxLayout:
        orientation: 'vertical'
        padding: '50sp', '200sp', '50sp', '200sp'
        spacing: '100sp'
        Buttons:
            text: 'To Enter'
            on_release: app.root.current = 'finances'
        Buttons:
            text: 'Quit'
            on_release: app.stop()
<Finances>:
    BoxLayout:
        orientation: 'vertical'
        ActionBar:
            ActionView:
                ActionPrevious:
                    title: 'Finances'
                    on_release: app.root.current = 'menu'
                ActionButton:
                    text: 'Menu'
        BoxLayout:
            orientation: 'vertical'
            padding: '50sp', '150sp', '50sp', '150sp'
            spacing: '50sp'
            Label:
                text: '1.000,00'
            BoxLayout:
                Label:
                    text: '1.500,00'
                Label:
                    text: '500,00'
            BoxLayout:
                spacing: '50sp'
                Buttons:
                    text: 'Receivement'
                Buttons:
                    text: 'Expenses'
            BoxLayout:
                padding: '10sp', '2sp', '10sp', '2sp'
                Buttons:
                    text: 'Refactor Base Value'
